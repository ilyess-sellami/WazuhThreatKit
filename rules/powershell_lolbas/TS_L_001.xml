<group name="powershell_lolbas">
  <rule id="TS_L_001" level="10">
    <decoded_as>windows_eventchannel</decoded_as>
    <field name="CommandLine">.*-EncodedCommand.*</field>
    <description>Possible LOL attack: PowerShell executed with encoded command</description>
    <mitre>
      <id>T1059.001</id>
      <tactic>Execution</tactic>
    </mitre>
  </rule>
</group>
