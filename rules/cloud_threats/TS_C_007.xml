<group name="cloud_threats">
  <rule id="TS_C_007" level="10">
    <decoded_as>json</decoded_as>
    <field name="command">.*gsutil rm -r gs://.*</field>
    <description>Suspicious GCP activity: bucket recursive deletion</description>
    <mitre>
      <id>T1531</id>
      <tactic>Impact</tactic>
    </mitre>
  </rule>
</group>
