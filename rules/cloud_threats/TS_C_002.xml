<group name="cloud_threats">
  <rule id="TS_C_002" level="10">
    <decoded_as>json</decoded_as>
    <field name="command">.*aws ec2 terminate-instances.*</field>
    <description>Suspicious AWS command: EC2 instance termination detected</description>
    <mitre>
      <id>T1531</id>
      <tactic>Impact</tactic>
    </mitre>
  </rule>
</group>
